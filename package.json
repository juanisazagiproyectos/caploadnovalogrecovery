{
    "name": "caploadnovalog",
    "version": "1.0.0",
    "description": "A simple CAP project.",
    "repository": "<Add your repository here>",
    "license": "UNLICENSED",
    "private": true,
    "dependencies": {
        "@sap/cds": "^6",
        "@sap/audit-logging": "^5",
        "@sap/xssec": "^3",
        "@sap/xsenv": "^3",
        "express": "^4",
        "hdb": "^0.19.0",
        "debug": "^4.3",
        "passport": "^0.6.0"
    },
    "devDependencies": {
        "@sap/ux-specification": "^1.102.9",
        "@types/passport": "^1.0.11",
        "sqlite3": "^5.0.4"
    },
    "scripts": {
        "start": "cds run",
        "watch-uimonitorloadnovalog": "cds watch --open uimonitorloadnovalog/webapp/index.html?sap-ui-xx-viewCache=false"
    },
    "engines": {
        "node": "^16.15"
    },
    "eslintConfig": {
        "extends": "eslint:recommended",
        "env": {
            "es2020": true,
            "node": true,
            "jest": true,
            "mocha": true
        },
        "globals": {
            "SELECT": true,
            "INSERT": true,
            "UPDATE": true,
            "DELETE": true,
            "CREATE": true,
            "DROP": true,
            "CDL": true,
            "CQL": true,
            "CXL": true,
            "cds": true
        },
        "rules": {
            "no-console": "off",
            "require-atomic-updates": "off"
        }
    },
    "cds": {
        "requires": {
            "uaa": {
                "kind": "xsuaa"
            },
            "auth": {
                "[production]": {
                    "strategy": "JWT"
                },
                "[development]": {
                    "strategy": "mock",
                    "users": {
                        "admin": {}
                    }
                }
            },
            "db": {
                "[production]": {
                    "kind": "hana-cloud"
                },
                "[development]": {
                    "kind": "sql"
                },
                "model": [
                    "db",
                    "srv"
                ]
            }
        }
    },
    "sapux": [
        "app/uimonitorloadnovalog"
    ]
}